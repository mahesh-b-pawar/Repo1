<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Insurance</display-name>
  <welcome-file-list>
    <welcome-file>Home.html</welcome-file>
    
  </welcome-file-list>
  
  <servlet>
  <servlet-name>reg</servlet-name>
  <servlet-class>com.insurance.Register</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>reg</servlet-name>
     <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  
  
  	<servlet>
	<servlet-name> logg</servlet-name>
	<servlet-class>com.insurance.Sample</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>logg</servlet-name>
	<url-pattern>/go</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>com.insurance.LoginDetails</servlet-class>
	<init-param>
	<param-name>email</param-name>
	<param-value>sne@gmail.com</param-value>
	</init-param>
	<init-param>
	<param-name>password</param-name>
	<param-value>qwerty</param-value>
	</init-param>
	</servlet>
	
	<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/logindetails</url-pattern>
	</servlet-mapping>
  
</web-app>